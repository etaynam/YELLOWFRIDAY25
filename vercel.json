{
  "crons": [
    {
      "path": "/api/cron/auto-post-question",
      "schedule": "*/3 * * * *"
    }
  ],
  "functions": {
    "api/cron/auto-post-question.js": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

